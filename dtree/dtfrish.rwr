% rewrite dtfrish {
  Main [YesNo] = ‛«YesNo»’
  YesNo [lb text y n rb] = ‛if («text») {⤷«y»⤶\n} else {⤷«n»⤶\n}’

  YesBranch [_or _yes _ x] = ‛\n«x»’
  NoBranch [_or _no _ x] = ‛\n«x»’

  text [cs+] = ‛«cs»’

  char_newline [_] = ‛’
  char_questionmark [_] = ‛’
  char_begindiv [_] = ‛’
  char_enddiv [_] = ‛’
  char_beginspan [lb cs* rb] = ‛’
  char_endspan [_] = ‛’
  char_at [_] = ‛%funcall ’
  char_space [_] = ‛’
  char_other [c] =‛«c»’
}
